{
  "name": "pgd-admin-panel",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "build": "BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js"
  },
  "license": "UNLICENSED",
  "keywords": [
    "inspinia",
    "react",
    "reactjs",
    "webpack"
  ],
  "author": "webapplayers",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-plugin-react": "^7.1.0",
    "estraverse-fb": "^1.3.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "animate.css": "^3.1.1",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "dom-tools": "^0.1.4",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jquery-slimscroll": "^1.3.6",
    "less": "^2.7.2",
    "lodash": "^4.17.4",
    "metismenu": "^2.5.0",
    "mobx": "^3.1.16",
    "mobx-react": "^4.2.2",
    "mobx-react-router": "^4.0.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.6.1",
    "react-pace": "^1.0.0",
    "react-router": "^4.1.1",
    "request": "^2.79.0"
  },
  "xo": {
    "space": true,
    "parser": "babel-eslint",
    "extends": "xo-react",
    "esnext": true,
    "env": [
      "node",
      "es6"
    ],
    "rules": {
      "eol-last": 0,
      "unicorn/filename-case": 0,
      "react/jsx-curly-spacing": [
        2,
        "always",
        {
          "spacing": {
            "objectLiterals": "never"
          }
        }
      ],
      "unicorn/no-abusive-eslint-disable": 0,
      "arrow-parens": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "operator-linebreak": [
        "error",
        "before"
      ],
      "import/no-unassigned-import": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "quote-props": [
        "error",
        "always"
      ]
    }
  }
}
