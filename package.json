{
  "name": "pgd-admin-panel",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "build": "BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js"
  },
  "keywords": [
    "inspinia",
    "react",
    "reactjs",
    "webpack"
  ],
  "author": "webapplayers",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.23.1",
    "eslint": "^3.15.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "animate.css": "^3.1.1",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "dom-tools": "^0.1.4",
    "font-awesome": "^4.7.0",
    "history": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jquery-slimscroll": "^1.3.6",
    "metismenu": "^2.5.0",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.6",
    "react-pace": "^1.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.79.0"
  },
  "xo": {
    "space": true,
    "parser": "babel-eslint",
    "esnext": true,
    "env": [
      "node",
      "es6"
    ],
    "rules": {
      "eol-last": 0,
      "unicorn/filename-case": 0,
      "arrow-parens": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "operator-linebreak": [
        "error",
        "before"
      ],
      "import/no-unassigned-import": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "quote-props": [
        "error",
        "always"
      ]
    }
  }
}
